{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "parse-down",
  "version": "0.1.0",
  "identifier": "com.parse-down.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Parse-down",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/16.png",
      "icons/32.png",
      "icons/128.png",
      "icons/192.png",
      "icons/512.png",
      "icons/icon.ico"
    ]
  }
}
